{
  "name": "steve-toolbox",
  "version": "0.0.1",
  "description": "A collection of React based components for my personal site.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --color --coverage --verbose",
    "lint": "eslint -c .eslintrc '**'",
    "watch": "webpack --config webpack.prod.js --mode=production --watch",
    "build": "webpack --config webpack.prod.js --mode=production"
  },
  "keywords": [
    "react",
    "toolbox"
  ],
  "author": "Steven Gibson <devgibsonsp@gmail.com>",
  "license": "GPL-3.0",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./test/shim.js",
      "./test/setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/test/.*.js$"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "jest": "^24.7.1",
    "jest-css-modules-transform": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "semantic-ui": "2.4.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {
    "es6-promise": "^4.2.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0"
  }
}
